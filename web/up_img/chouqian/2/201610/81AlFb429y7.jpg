    <script type="text/javascript">
        var aim = 5;
        function clearInput(id)
        {
            document.getElementById(id).value = '';
        }
        function setDate()
        {
            var dd = new Date();
            // 获取AddDayCount天后的日期
            var y = dd.getFullYear();
            var m = dd.getMonth()+1;//获取当前月份的日期
            var d = dd.getDate();
            if(m < 10){
                m = '0' + m;
            }
            if(d < 10){
                d = '0' + d;
            }
            var dd = y+"-"+m+"-"+d;
            document.getElementById('hideDate').value = dd;
        }

        function check_form()
        {
            var v = document.getElementById('post').value;
            if(v == '' || v == '请输入梦境')
            {
                alert('请输入梦境');
                return false;
            }
            $('#form1').submit();
            return true;
        }

        function check_enter(event)
        {
            if (event.keyCode == 13)
            {
                return false;
            }
        }

        function check_value()
        {
            var v = document.getElementById('post').value;
            if (v == '')
            {
                return false;
            }
            else
            {
                return true;
            }

        }
        function chkmobile(e)
        {
            if(e == 'caipu')
            {
                var caipu = document.getElementById('caipu');
                if(!caipu.value || caipu.value == '请输入菜谱名称')
                {
                    alert("请输入菜谱名称");
                    return false;
                }

                $('#caipuFrom').submit();
                return false;
            }

            if(e == 'sfzcx')
            {
                var sfzcx = document.getElementById('sfzcx');
                if(chkCard(sfzcx.value) == false || sfzcx.value == '15位或18位号码'){
                    alert('请输入15或18位正确的身份证号码');
                    return false;
                }
                $('#sfzcxFrom').submit();
                return false;
            }
            var mobile = document.getElementById('mobile');
            if(!mobile.value || mobile.value == '请输入菜谱名称')
            {
                alert("请输入手机号");
                return false;
            }
            var par = /^ *[0-9]{7,11} *$/;
            if(!par.test(mobile.value))
            {
                alert("请输入正确的手机号码");
                return false;
            }
            val = mobile.value.replace(/(^\s*)|(\s*$)/g, "");
            if(arguments[0] && arguments[0] == 1){
                document.getElementById("shouji").action = 'http://tools.2345.com/shouji.htm?m=' + val;
                document.getElementById("shouji").target = '_blank';
            }else{
                window.open( 'http://tools.2345.com/shouji.htm?m=' + val);
            }

        }

        function chkCard(card) {
            if(card.length == 15 || card.length == 18){

                var tmp = card;
                if(card.length == 18) {
                    card = tmp.substr(0,17);
                }
                if(!isNaN(card)) {
                    return true;
                }
            }
            return false;
        }
